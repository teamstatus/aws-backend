{
  "name": "@teamstatus/aws-backend",
  "version": "0.0.0-development",
  "description": "AWS backend for teamstatus developed using CDK in TypeScript",
  "type": "module",
  "scripts": {
    "test": "find ./ -type f -name '*.spec.ts' -not -path './node_modules/*' -print | xargs npx tsx --no-warnings --test --test-reporter spec",
    "prepare": "husky install && check-node-version --package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teamstatus/aws-backend.git"
  },
  "bugs": {
    "url": "https://github.com/teamstatus/aws-backend/issues"
  },
  "homepage": "https://github.com/teamstatus/aws-backend#readme",
  "keywords": [
    "teamstatus",
    "backend",
    "aws",
    "cdk"
  ],
  "author": "Markus Tacker <m@coderbyheart.com> | coderbyheart.com",
  "license": "MIT",
  "devDependencies": {
    "@aws-sdk/client-cloudformation": "3.428.0",
    "@aws-sdk/client-dynamodb": "3.428.0",
    "@aws-sdk/client-iam": "3.428.0",
    "@aws-sdk/client-s3": "3.428.0",
    "@aws-sdk/client-ses": "3.428.0",
    "@aws-sdk/client-sns": "3.428.0",
    "@aws-sdk/client-ssm": "3.428.0",
    "@aws-sdk/s3-request-presigner": "3.428.0",
    "@aws-sdk/util-dynamodb": "3.428.0",
    "@biomejs/biome": "1.2.2",
    "@commitlint/config-conventional": "17.8.0",
    "@nordicsemiconductor/cloudformation-helpers": "8.0.0",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.93",
    "@types/aws-lambda": "8.10.124",
    "@types/glob": "8.1.0",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/mailparser": "3.4.2",
    "@types/yazl": "2.4.3",
    "aws-cdk": "2.101.0",
    "aws-cdk-lib": "2.101.0",
    "cdk": "2.101.0",
    "chalk": "5.3.0",
    "check-node-version": "4.2.1",
    "commitlint": "17.8.0",
    "constructs": "10.3.0",
    "glob": "10.3.10",
    "husky": "8.0.3",
    "prettier": "3.0.3",
    "tsmatchers": "5.0.0",
    "tsx": "3.13.0",
    "typescript": "5.2.2",
    "ulid": "2.3.0",
    "yazl": "2.5.1"
  },
  "lint-staged": {
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "npx biome format --write",
      "npx biome check"
    ]
  },
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/github",
        {
          "successComment": false,
          "failTitle": false
        }
      ]
    ]
  },
  "dependencies": {
    "@nordicsemiconductor/from-env": "2.0.0",
    "jsonwebtoken": "9.0.2",
    "mailparser": "3.6.5"
  }
}
