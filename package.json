{
  "name": "@teamstatus/aws-backend",
  "version": "0.0.0-development",
  "description": "AWS backend for teamstatus developed using CDK in TypeScript",
  "type": "module",
  "scripts": {
    "test": "npx tsx --test --test-reporter spec ./**/**/*.spec.ts",
    "test:e2e": "$(set -o pipefail && npx tsx ./feature-runner/run-features.ts | npx tsx ./feature-runner/console-reporter.ts)",
    "prepare": "husky install && check-node-version --package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teamstatus/aws-backend.git"
  },
  "bugs": {
    "url": "https://github.com/teamstatus/aws-backend/issues"
  },
  "homepage": "https://github.com/teamstatus/aws-backend#readme",
  "keywords": [
    "teamstatus",
    "backend",
    "aws",
    "cdk"
  ],
  "author": "Markus Tacker <m@coderbyheart.com> | coderbyheart.com",
  "license": "MIT",
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "3.362.0",
    "@aws-sdk/client-iam": "3.362.0",
    "@aws-sdk/client-ses": "3.362.0",
    "@aws-sdk/client-ssm": "3.362.0",
    "@aws-sdk/util-dynamodb": "3.362.0",
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "12.0.48",
    "@nordicsemiconductor/bdd-markdown": "3.6.23",
    "@nordicsemiconductor/cloudformation-helpers": "8.0.0",
    "@nordicsemiconductor/eslint-config-asset-tracker-cloud-typescript": "15.0.11",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.67",
    "@types/aws-lambda": "8.10.119",
    "@types/glob": "8.1.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/yazl": "2.4.2",
    "@typescript-eslint/eslint-plugin": "5.60.1",
    "aws-cdk": "2.85.0",
    "aws-cdk-lib": "2.85.0",
    "cdk": "2.85.0",
    "chalk": "5.2.0",
    "check-node-version": "4.2.1",
    "constructs": "10.2.64",
    "eslint-plugin-unicorn": "47.0.0",
    "glob": "10.3.1",
    "husky": "8.0.3",
    "prettier": "2.8.8",
    "tsmatchers": "4.0.12",
    "tsx": "3.12.7",
    "typescript": "5.1.6",
    "ulid": "2.3.0",
    "yazl": "2.5.1"
  },
  "lint-staged": {
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ]
  },
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/github",
        {
          "successComment": false,
          "failTitle": false
        }
      ]
    ]
  },
  "prettier": "@nordicsemiconductor/asset-tracker-cloud-code-style/.prettierrc",
  "dependencies": {
    "@nordicsemiconductor/from-env": "2.0.0",
    "jsonwebtoken": "9.0.0"
  }
}
