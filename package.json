{
  "name": "@teamstatus/aws-backend",
  "version": "0.0.0-development",
  "description": "AWS backend for teamstatus developed using CDK in TypeScript",
  "type": "module",
  "scripts": {
    "test": "find ./ -type f -name \\*.spec.ts | xargs npx tsx --test --test-reporter spec",
    "test:e2e": "npx tsx ./feature-runner/run-features.ts | npx tsx ./feature-runner/console-reporter.ts",
    "prepare": "husky install && check-node-version --package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teamstatus/aws-backend.git"
  },
  "bugs": {
    "url": "https://github.com/teamstatus/aws-backend/issues"
  },
  "homepage": "https://github.com/teamstatus/aws-backend#readme",
  "keywords": [
    "teamstatus",
    "backend",
    "aws",
    "cdk"
  ],
  "author": "Markus Tacker <m@coderbyheart.com> | coderbyheart.com",
  "license": "MIT",
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "3.379.1",
    "@aws-sdk/client-iam": "3.379.1",
    "@aws-sdk/client-s3": "3.379.1",
    "@aws-sdk/client-ses": "3.379.1",
    "@aws-sdk/client-sns": "3.379.1",
    "@aws-sdk/client-ssm": "3.379.1",
    "@aws-sdk/s3-request-presigner": "3.379.1",
    "@aws-sdk/util-dynamodb": "3.379.1",
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "12.0.64",
    "@nordicsemiconductor/bdd-markdown": "5.6.0",
    "@nordicsemiconductor/cloudformation-helpers": "8.0.0",
    "@nordicsemiconductor/eslint-config-asset-tracker-cloud-typescript": "16.0.8",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.72",
    "@types/aws-lambda": "8.10.119",
    "@types/glob": "8.1.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mailparser": "3.4.0",
    "@types/yazl": "2.4.2",
    "@typescript-eslint/eslint-plugin": "6.2.0",
    "aws-cdk": "2.89.0",
    "aws-cdk-lib": "2.89.0",
    "cdk": "2.89.0",
    "chalk": "5.3.0",
    "check-node-version": "4.2.1",
    "constructs": "10.2.69",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-unicorn": "48.0.1",
    "glob": "10.3.3",
    "husky": "8.0.3",
    "prettier": "3.0.0",
    "tsmatchers": "4.0.12",
    "tsx": "3.12.7",
    "typescript": "5.1.6",
    "ulid": "2.3.0",
    "yazl": "2.5.1"
  },
  "lint-staged": {
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ]
  },
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/github",
        {
          "successComment": false,
          "failTitle": false
        }
      ]
    ]
  },
  "prettier": "@nordicsemiconductor/asset-tracker-cloud-code-style/.prettierrc",
  "dependencies": {
    "@nordicsemiconductor/from-env": "2.0.0",
    "jsonwebtoken": "9.0.1",
    "mailparser": "3.6.5"
  }
}
